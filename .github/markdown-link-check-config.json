{
  "ignorePatterns": [
    {
      "pattern": "http://localhost*"
    },
    {
      "pattern": "https://localhost*"
    },
    {
      "pattern": "*.local"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHttpStatus": 200,
  "aliveStatusCodes": [200, 206, 301, 302, 308]
}
