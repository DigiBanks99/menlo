{
  "local": {
    "baseUrl": "https://localhost:7033/api",
    "Security": {
      "Auth": {
        "Api": {
          "Type": "OAuth2",
          "Grant Type": "Client Credentials",
          "Client ID": "{{ClientId}}",
          "Client Secret": "{{ClientSecret}}",
          "Token URL": "https://login.microsoftonline.com/{{TenantId}}/oauth2/v2.0/token",
          "Scope": "{{Scope}}"
        }
      }
    }
  },
  "local-docker": {
    "baseUrl": "https://localhost:5001/api"
  }
}
