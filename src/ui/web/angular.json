{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "menlo-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/menlo-app",
      "sourceRoot": "projects/menlo-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/menlo-app/src/main.ts",
            "tsConfig": "projects/menlo-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/menlo-app/public"
              }
            ],
            "styles": [
              "projects/menlo-app/src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "menlo-app:build:production"
            },
            "development": {
              "buildTarget": "menlo-app:build:development",
              "proxyConfig": "proxy.conf.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/menlo-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/menlo-app/public"
              }
            ],
            "styles": [
              "projects/menlo-app/src/styles.scss"
            ]
          }
        },
        "storybook": {
          "builder": "@analogjs/storybook-angular:start-storybook",
          "options": {
            "configDir": "projects/menlo-app/.storybook",
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@analogjs/storybook-angular:build-storybook",
          "options": {
            "configDir": "projects/menlo-app/.storybook",
            "outputDir": "dist/storybook/menlo-app"
          }
        }
      }
    },
    "shared-util": {
      "projectType": "library",
      "root": "projects/shared-util",
      "sourceRoot": "projects/shared-util/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/shared-util/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shared-util/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/shared-util/tsconfig.spec.json"
          }
        }
      }
    },
    "data-access-menlo-api": {
      "projectType": "library",
      "root": "projects/data-access-menlo-api",
      "sourceRoot": "projects/data-access-menlo-api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/data-access-menlo-api/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/data-access-menlo-api/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/data-access-menlo-api/tsconfig.spec.json"
          }
        }
      }
    },
    "menlo-lib": {
      "projectType": "library",
      "root": "projects/menlo-lib",
      "sourceRoot": "projects/menlo-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/menlo-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/menlo-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/menlo-lib/tsconfig.spec.json"
          }
        },
        "storybook": {
          "builder": "@analogjs/storybook-angular:start-storybook",
          "options": {
            "configDir": "projects/menlo-lib/.storybook",
            "port": 6007
          }
        },
        "build-storybook": {
          "builder": "@analogjs/storybook-angular:build-storybook",
          "options": {
            "configDir": "projects/menlo-lib/.storybook",
            "outputDir": "dist/storybook/menlo-lib"
          }
        }
      }
    }
  }
}
