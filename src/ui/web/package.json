{
  "name": "menlo-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nx serve menlo-app",
    "start:prod": "nx serve menlo-app --configuration=production",
    "build": "nx build menlo-app",
    "build:prod": "nx build menlo-app --configuration=production",
    "build:all": "nx run-many --target=build --all",
    "build:all:prod": "nx run-many --target=build --all --configuration=production",
    "test": "nx test menlo-app",
    "test:all": "nx run-many --target=test --all",
    "test:affected": "nx affected --target=test",
    "watch": "nx build menlo-app --watch",
    "storybook": "nx storybook menlo-app",
    "build-storybook": "nx build-storybook menlo-app",
    "storybook:lib": "nx storybook menlo-lib",
    "build-storybook:lib": "nx build-storybook menlo-lib",
    "graph": "nx graph",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "lint": "nx run-many --target=lint --all",
    "lint:affected": "nx affected --target=lint",
    "preview": "nx build menlo-app --configuration=production && npx http-server dist/menlo-app -p 4200",
    "health-check": "curl -f http://localhost:4200/ || exit 1"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^20.1.6",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-browser-dynamic": "^20.3.1",
    "@angular/router": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@analogjs/storybook-angular": "^1.20.1",
    "@analogjs/vitest-angular": "^1.21.1",
    "@angular-devkit/core": "^20.1.6",
    "@angular-devkit/schematics": "^20.1.6",
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.0",
    "@compodoc/compodoc": "^1.1.26",
    "@nx/angular": "21.5.3",
    "@nx/js": "21.5.3",
    "@nx/vite": "^21.5.3",
    "@nx/web": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@schematics/angular": "^20.1.6",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/angular": "^9.1.3",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jasmine": "~5.1.0",
    "@types/node": "20.19.9",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "jasmine-core": "~5.8.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "nx": "21.5.3",
    "prettier": "^2.6.2",
    "storybook": "^9.1.3",
    "typescript": "~5.8.2",
    "vite": "^7.1.6",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^3.2.4"
  }
}
