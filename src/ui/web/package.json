{
  "name": "menlo-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve menlo-app",
    "build": "ng build menlo-app",
    "watch": "ng build menlo-app --watch --configuration development",
    "test": "ng test menlo-app",
    "build:all": "ng build menlo-app && ng build menlo-lib && ng build shared-util && ng build data-access-menlo-api",
    "storybook": "ng run menlo-app:storybook",
    "build-storybook": "ng run menlo-app:build-storybook",
    "storybook:lib": "ng run menlo-lib:storybook",
    "build-storybook:lib": "ng run menlo-lib:build-storybook"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^20.1.6",
    "@angular/animations": "^20.2.3",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@analogjs/storybook-angular": "^1.20.1",
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.0",
    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/angular": "^9.1.3",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "storybook": "^9.1.3",
    "typescript": "~5.8.2"
  }
}
