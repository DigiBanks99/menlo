{
  "name": "menlo-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nx serve menlo-app",
    "start:prod": "nx serve menlo-app --configuration=production",
    "build": "nx build menlo-app",
    "build:prod": "nx build menlo-app --configuration=production",
    "build:all": "nx run-many --target=build --all",
    "build:all:prod": "nx run-many --target=build --all --configuration=production",
    "test": "nx test menlo-app",
    "test:all": "nx run-many --target=test --all",
    "test:affected": "nx affected --target=test",
    "watch": "nx build menlo-app --watch",
    "storybook": "nx storybook menlo-app",
    "build-storybook": "nx build-storybook menlo-app",
    "storybook:lib": "nx storybook menlo-lib",
    "build-storybook:lib": "nx build-storybook menlo-lib",
    "graph": "nx graph",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "lint": "nx run-many --target=lint --all",
    "lint:affected": "nx affected --target=lint",
    "preview": "nx build menlo-app --configuration=production && npx http-server dist/menlo-app -p 4200",
    "health-check": "curl -f http://localhost:4200/ || exit 1"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^20.3.11",
    "@angular/animations": "^20.3.13",
    "@angular/common": "^20.3.13",
    "@angular/compiler": "^20.3.13",
    "@angular/core": "^20.3.13",
    "@angular/forms": "^20.3.13",
    "@angular/platform-browser": "^20.3.13",
    "@angular/platform-browser-dynamic": "^20.3.13",
    "@angular/router": "^20.3.13",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@analogjs/storybook-angular": "^1.22.5",
    "@analogjs/vite-plugin-angular": "^1.22.5",
    "@analogjs/vitest-angular": "^1.22.5",
    "@angular-devkit/core": "^20.3.11",
    "@angular-devkit/schematics": "^20.3.11",
    "@angular-eslint/eslint-plugin": "^20.7.0",
    "@angular-eslint/eslint-plugin-template": "^20.7.0",
    "@angular-eslint/template-parser": "^20.7.0",
    "@angular/build": "^20.3.11",
    "@angular/cli": "^20.3.11",
    "@angular/compiler-cli": "^20.3.13",
    "@compodoc/compodoc": "^1.1.32",
    "@eslint/eslintrc": "^3.3.1",
    "@nx/angular": "21.5.3",
    "@nx/eslint": "^21.6.9",
    "@nx/eslint-plugin": "^21.6.9",
    "@nx/js": "21.5.3",
    "@nx/vite": "^21.6.9",
    "@nx/web": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@schematics/angular": "^20.3.11",
    "@storybook/addon-docs": "^9.1.16",
    "@storybook/angular": "^9.1.16",
    "@swc-node/register": "~1.9.2",
    "@swc/core": "~1.5.29",
    "@swc/helpers": "~0.5.17",
    "@types/node": "20.19.9",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.39.1",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "ng-packagr": "^20.3.2",
    "nx": "21.5.3",
    "prettier": "^2.8.8",
    "storybook": "^9.1.16",
    "typescript": "~5.8.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "~4.5.4",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "koa@>=3.0.1 <3.0.3": ">=3.0.3"
    }
  }
}
