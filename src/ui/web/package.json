{
  "name": "menlo-web",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "build-storybook:lib": "nx build-storybook menlo-lib",
    "build-storybook": "nx build-storybook menlo-app",
    "build:all:prod": "nx run-many --target=build --all --configuration=production",
    "build:all": "nx run-many --target=build --all",
    "build:prod": "nx build menlo-app --configuration=production",
    "build": "nx build menlo-app",
    "format": "nx format:write",
    "graph": "nx graph",
    "health-check": "curl -f http://localhost:4200/ || exit 1",
    "lint:affected": "nx affected --target=lint",
    "lint": "nx run-many --target=lint --all",
    "ng": "ng",
    "preview": "nx build menlo-app --configuration=production && npx http-server dist/menlo-app -p 4200",
    "start:prod": "nx serve menlo-app --configuration=production",
    "start": "nx serve menlo-app",
    "storybook:lib": "nx storybook menlo-lib",
    "storybook": "nx storybook menlo-app",
    "test:affected": "nx affected --target=test",
    "test:all": "nx run-many --target=test --all -- --coverage",
    "test": "nx test menlo-app",
    "watch": "nx build menlo-app --watch"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^21.1.2",
    "@angular/animations": "^21.1.1",
    "@angular/common": "^21.1.1",
    "@angular/compiler": "^21.1.1",
    "@angular/core": "^21.1.1",
    "@angular/forms": "^21.1.1",
    "@angular/platform-browser": "^21.1.1",
    "@angular/platform-browser-dynamic": "^21.1.1",
    "@angular/router": "^21.1.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "vite": "7.3.1",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@analogjs/storybook-angular": "^2.2.3",
    "@analogjs/vite-plugin-angular": "^2.2.3",
    "@analogjs/vitest-angular": "^2.2.3",
    "@angular-devkit/core": "^21.1.2",
    "@angular-devkit/schematics": "^21.1.2",
    "@angular-eslint/eslint-plugin": "^21.2.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0",
    "@angular-eslint/template-parser": "^21.2.0",
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^21.1.1",
    "@compodoc/compodoc": "^1.2.1",
    "@eslint/eslintrc": "^3.3.3",
    "@nx/angular": "22.4.1",
    "@nx/eslint": "22.4.1",
    "@nx/eslint-plugin": "22.4.1",
    "@nx/js": "22.4.1",
    "@nx/vite": "22.4.1",
    "@nx/web": "22.4.1",
    "@nx/workspace": "22.4.1",
    "@schematics/angular": "^21.1.2",
    "@storybook/addon-docs": "^10.2.3",
    "@storybook/angular": "^10.2.3",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.11",
    "@swc/helpers": "~0.5.18",
    "@types/node": "25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "10.2.3",
    "jiti": "2.6.1",
    "jsdom": "~27.4.0",
    "ng-packagr": "^21.1.0",
    "nx": "22.4.4",
    "prettier": "^3.8.1",
    "storybook": "^10.2.3",
    "typescript": "~5.9.3",
    "vite-plugin-dts": "~4.5.4",
    "vitest": "^4.0.18"
  }
}
