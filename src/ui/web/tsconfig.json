{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "paths": {
      "data-access-menlo-api": ["projects/data-access-menlo-api/src/public-api.ts"],
      "menlo-lib": ["projects/menlo-lib/src/public-api.ts"],
      "shared-util": ["projects/shared-util/src/public-api.ts"]
    },
    "importHelpers": true,
    "target": "ES2022",
    "module": "preserve"
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "typeCheckHostBindings": true,
    "strictTemplates": true
  },
  "files": [],
  "references": [
    {
      "path": "./projects/menlo-app/tsconfig.app.json"
    },
    {
      "path": "./projects/menlo-app/tsconfig.spec.json"
    },
    {
      "path": "./projects/shared-util/tsconfig.lib.json"
    },
    {
      "path": "./projects/shared-util/tsconfig.spec.json"
    },
    {
      "path": "./projects/data-access-menlo-api/tsconfig.lib.json"
    },
    {
      "path": "./projects/data-access-menlo-api/tsconfig.spec.json"
    },
    {
      "path": "./projects/menlo-lib/tsconfig.lib.json"
    },
    {
      "path": "./projects/menlo-lib/tsconfig.spec.json"
    }
  ]
}
